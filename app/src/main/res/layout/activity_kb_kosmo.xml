<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/deep_blue"
    tools:context=".knowledge.KBKosmoActivity">

    <ImageView
        android:id="@+id/kb_kosmo_zrcadlo1"
        android:layout_width="52dp"
        android:layout_height="150dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="-8dp"
        app:layout_constraintBottom_toTopOf="@+id/kb_kosmo_arr1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:srcCompat="@drawable/rect"
        app:tint="@color/background" />

    <ImageView
        android:id="@+id/kb_kosmo_zrcadlo2"
        android:layout_width="3dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintEnd_toEndOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintTop_toTopOf="@+id/kb_kosmo_zrcadlo1"
        app:srcCompat="@drawable/rect"
        app:tint="@color/iqprimary" />

    <ImageView
        android:id="@+id/kb_kosmo_zrcadlo3"
        android:layout_width="0dp"
        android:layout_height="3dp"
        app:layout_constraintEnd_toEndOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintTop_toTopOf="@+id/kb_kosmo_zrcadlo1"
        app:srcCompat="@drawable/rect"
        app:tint="@color/iqprimary" />

    <ImageButton
        android:id="@+id/kb_kosmo_zrcadloq"
        android:layout_width="0dp"
        android:layout_height="64sp"
        android:background="@drawable/empty_space"
        android:contentDescription="@string/kb_kosmo_zrcadlo"
        android:scaleType="fitCenter"
        android:src="@drawable/google_round_question_mark_24"
        app:layout_constraintBottom_toBottomOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintEnd_toEndOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintStart_toStartOf="@+id/kb_kosmo_zrcadlo1"
        app:layout_constraintTop_toTopOf="@+id/kb_kosmo_zrcadlo1" />

    <ImageView
        android:id="@+id/kb_kosmo_arr1"
        android:layout_width="70sp"
        android:layout_height="70sp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="8dp"
        android:rotation="-90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/kb_kosmo_wall1"
        app:srcCompat="@drawable/google_baseline_arrow_forward_24" />

    <ImageView
        android:id="@+id/kb_kosmo_wall1"
        android:layout_width="248sp"
        android:layout_height="78sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/kb_kosmo_arr1"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/rect"
        app:tint="@color/background" />

    <ImageView
        android:id="@+id/kb_kosmo_roboruka"
        android:layout_width="64sp"
        android:layout_height="64sp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/kb_kosmo_wall1"
        app:layout_constraintEnd_toEndOf="@+id/kb_kosmo_wall1"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="@+id/kb_kosmo_wall1"
        app:srcCompat="@drawable/google_round_question_mark_24" />

    <ImageView
        android:id="@+id/kb_kosmo_rover"
        android:layout_width="64sp"
        android:layout_height="64sp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="64dp"
        app:layout_constraintBottom_toTopOf="@+id/kb_kosmo_wall1"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/google_round_question_mark_24" />
</androidx.constraintlayout.widget.ConstraintLayout>