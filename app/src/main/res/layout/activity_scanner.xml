                android:id="@+id/show_starts_in_static"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/normal"
                android:text="@string/scanner_static_starts_in"
                android:textColor="@color/unset_fg"
                app:layout_constraintStart_toStartOf="@+id/show_name_static"
                app:layout_constraintTop_toBottomOf="@+id/show_name_static" />

            <TextView
                android:id="@+id/show_starts_in"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/normal"
                android:text="@string/loading"
                android:textColor="@color/unset_fg"
                app:layout_constraintBottom_toBottomOf="@+id/show_starts_in_static"
                app:layout_constraintStart_toEndOf="@+id/show_starts_in_static"
                app:layout_constraintTop_toTopOf="@+id/show_starts_in_static" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>