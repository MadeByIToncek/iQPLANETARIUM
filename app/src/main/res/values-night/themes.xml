<resources>
    <!-- Base application theme. -->
    <style name="Theme.IQPLANETARIUM" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/iqprimary</item>
        <item name="colorPrimaryVariant">@color/iqprimary_alt</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/iqprimary_drk</item>
        <item name="colorSecondaryVariant">@color/iqprimary_altdrk</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <item name="android:background">@color/background</item>
        <!-- Customize your theme here. -->
    </style>
</resources>